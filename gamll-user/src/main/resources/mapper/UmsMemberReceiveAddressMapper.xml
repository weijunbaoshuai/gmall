<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gmall.gmalluser.mapper.UmsMemberReceiveAddressMapper">
    <select id="selectByMemberId" resultType="com.gmall.gmalluser.bean.UmsReceiveAddress" parameterType="string">
        select * from ums_member_receive_address where member_id=#{memberId}
